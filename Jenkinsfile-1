pipeline {
  agent {
    docker { image 'node:16-alpine' }
  }
  environment {
        HOME = '.'
    }
  stages {
    stage('Test') {
      steps {
        sh 'node --version'
        sh 'npm -v' 
        sh 'ls -lart' 
        sh 'npm install --cache npm_cache'
        sh 'npm audit fix --force'
      }
    }
  }
}
